@model List<AppUser>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
    int count = 0;
}


<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Ad</th>
        <th>Soyad</th>
        <th>Rolleri Gör</th>
    </tr>
    @foreach (var item in Model)
    {
        count++;
        <tr>
            <th>@count</th>
            <th>@item.Name</th>
            <td>@item.Surname</td>
            <td><a href="/AdminRoleUser/GetRole/@item.Id" class="btn btn-outline-dark">Role Git</a></td>
        </tr>
    }
</table>
<br />
<a href="/AdminRoleUser/AddRoleUser/" class="btn btn-outline-success"> Yeni Rol ekle</a>



