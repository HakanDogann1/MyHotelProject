@model List<GuestListViewModel>
<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <div class="active-member">
                    <div class="table-responsive">
                        <table class="table table-xs mb-0">
                            <h3>Son 6 Müşteri</h3>
                            <thead>
                                <tr>
                                    <th>Ad</th>
                                    <th>Soyad</th>
                                    <th>Şehir</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>@item.GuestName</td>
                                        <td>@item.GuestSurname</td>
                                        <td>
                                            <span>@item.GuestCity</span>
                                        </td>
                                    </tr>
                                }
                              
                               
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>